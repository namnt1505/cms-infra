{
  "name": "cms-infra",
  "version": "0.1.0",
  "bin": {
    "cms-infra": "bin/cms-infra.js"
  },
  "scripts": {
    "cms:bootstrap": "cdk bootstrap --toolkit-stack-name CmsCdkToolkit --qualifier cmsapp --timeout 600",
    "cms:synth": "cdk synth --qualifier cmsapp",
    "cms:diff": "cdk diff --qualifier cmsapp",
    "cms:deploy": "cdk deploy --qualifier cmsapp",
    "cms:destroy": "cdk destroy --qualifier cmsapp",
    "sniper:bootstrap": "cdk bootstrap --toolkit-stack-name SniperToolkit --qualifier sniper-bot --timeout 600",
    "sniper:synth": "TARGET=sniper-bot cdk synth --qualifier sniper-bot",
    "sniper:diff": "TARGET=sniper-bot cdk synth --qualifier sniper-bot",
    "sniper:deploy": "TARGET=sniper-bot cdk deploy --qualifier sniper-bot",
    "sniper:destroy": "TARGET=sniper-bot cdk destroy --qualifier sniper-bot",
    "msq:bootstrap": "cdk bootstrap --toolkit-stack-name MsqToolkit --qualifier ms-queue --timeout 600",
    "msq:synth": "TARGET=msq cdk synth --qualifier ms-queue",
    "msq:diff": "TARGET=msq cdk synth --qualifier ms-queue",
    "msq:deploy": "TARGET=msq cdk deploy --qualifier ms-queue",
    "msq:destroy": "TARGET=msq cdk destroy --qualifier ms-queue"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "20.12.7",
    "aws-cdk": "2.160.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.160.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "constructs": "^10.3.0",
    "fs-extra": "^11.2.0",
    "jsonschema": "^1.4.1",
    "source-map-support": "^0.5.21"
  },
  "packageManager": "pnpm@9.2.0+sha1.886364ebd6dae009284bebc56ed4296bb6dd557a"
}
